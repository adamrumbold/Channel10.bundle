<?xml version="1.0" encoding="utf-8"?>

<site site="http://ten.com.au/.*"
    plugin="http://apps.v2.movideo.com/player/flash/movideo_player.swf"
 	initialState="playing"
 	version="1.0">

<crop x="0" y="0" width="512" height="288" />

<seekbar type="simple">
 <start x="48" y="308" />
 <end   x="185" y="308" />
 <played>
   <condition>
    <command name="play" />
    </condition>
  </played>

</seekbar>

<state name="playing">
  <event>
   <condition>
    <command name="pause" />
   </condition>
   <action>
    <click x="19" y="301" />
    <goto state="paused" />
   </action>
  </event>
 </state>

 <state name="paused">
  <event>
   <condition>
    <command name="play" />
   </condition>
   <action>
    <click x="19" y="301" />
    <goto state="playing" />
   </action>
  </event>
 </state>

</site>
